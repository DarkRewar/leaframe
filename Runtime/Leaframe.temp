    z-index: 1;
    border-radius: 5px;
    box-shadow: 0 5px 7px -5px #000;
    padding: 2px;
    border-width: 0 1px;
    border-color: #EEE;
    border-style: solid; }
.block-pres section > * {
    max-width: 100%; }
.block-pres footer {
    margin-top: -30px;
    transition: 1s ease-in-out; }
.block-pres footer .expand {
    width: 100%;
    color: #FFF;
    font-size: 1.5em;
    padding: 7px 0 2px 0;
    border: none;
    border-radius: 0 0 5px 5px;
    margin: 0; }

[data-drop] {
    width: auto;
    display: inline; }
[data-drop] a.ref {
    display: flex;
    position: relative;
    height: auto;
    padding-right: 35px !important;
    line-height: 42px; }
[data-drop] a.ref:after {
    display: flex;
    position: absolute;
    content: "";
    margin-top: -23px;
    margin-right: 16px;
    right: 0;
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 5px 5px 0 5px;
    border-color: #FFF transparent transparent transparent; }
[data-drop]:hover {
    background-color: #222; }

.drop {
    display: none;
    position: absolute;
    background-color: #333;
    top: auto;
    overflow: hidden;
    margin-top: 42px;
    border-top: 1px solid #DDD; }
.drop ul.beauty {
    padding: 0; }
.drop ul.beauty li:first-child {
    border-top: 0; }
.drop ul.beauty li:last-child {
    border-bottom: 0; }
.drop ul.beauty li a:hover {
    background-color: #222;
    text-shadow: 0 0 0; }
.drop > ul > li {
    float: none; }
.drop > ul > li > a {
    float: none; }

