/* Custom CSS via div.progress-bar */
.progress-bar {
    display: flex;
    background-size: auto;
    height: 20px;
    background-color: #F1F1F1;
    overflow: hidden; }
.progress-bar.fat {
    height: 26px; }
.progress-bar.small {
    height: 14px;
    font-size: inherit; }
.progress-bar.tiny {
    height: 8px; }
.progress-bar .progress-value {
    display: flex;
    min-width: 0%;
    width: 0%;
    max-width: 100%;
    height: 100%;
    background-color: #dddddd;
    -webkit-transition: width 1s cubic-bezier(0.2, 0.1, 0, 1);
    -moz-transition: width 1s cubic-bezier(0.2, 0.1, 0, 1);
    -o-transition: width 1s cubic-bezier(0.2, 0.1, 0, 1);
    transition: width 1s cubic-bezier(0.2, 0.1, 0, 1); }
.progress-bar.success .progress-value {
    background-color: #2ecc71; }
.progress-bar.error .progress-value {
    background-color: #e74c3c; }
.progress-bar.info .progress-value {
    background-color: #3498db; }
.progress-bar.warning .progress-value {
    background-color: #f39c12; }
.progress-bar.rounded .progress-value, .breadcrumbs.arrow.rounded ul.progress-bar .progress-value {
    border-radius: 5px; }
.progress-bar.round .progress-value {
    border-radius: 35px; }
.progress-bar.stripped .progress-value {
    background-image: -webkit-linear-gradient(bottom left, rgba(255, 255, 255, 0.15) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, 0.15) 50%, rgba(255, 255, 255, 0.15) 75%, transparent 75%, transparent);
    background-image: -o-linear-gradient(bottom left, rgba(255, 255, 255, 0.15) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, 0.15) 50%, rgba(255, 255, 255, 0.15) 75%, transparent 75%, transparent);
    background-image: linear-gradient(to top right, rgba(255, 255, 255, 0.15) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, 0.15) 50%, rgba(255, 255, 255, 0.15) 75%, transparent 75%, transparent);
    -webkit-background-size: 40px 40px;
    background-size: 40px 40px; }

.message {
    display: flex;
    width: 100%;
    color: #111;
    padding: 15px 42px 15px 22px;
    font-size: 18px;
    margin: 4px 0;
    position: relative;
    border-bottom: 3px solid #eee;
    color: #fff;
    -unity-font-style: bold; }
.message h1, .message h2, .message h3, .message h4, .message h5, .message h6 {
    padding: 0;
    margin: 0; }
.message p {
    -unity-font-style: normal; }
.message a.close, .message .theater .theater-scene a.close, .theater .theater-scene .message a.close {
    top: 12px;
    font-size: 25px;
    color: rgba(0, 0, 0, 0.5); }
.message.error {
    background-color: #ff5442;
    border-color: #8f2f25; }
.message.success {
    border-color: #1d9650; }
.message.info {
    border-color: #1f78b3; }
.message.warning {
    border-color: #b7760e; }

a.close, .theater .theater-scene a.close {
    content: "&times;";
    position: absolute;
    right: 0;
    margin-right: 14px;
    cursor: pointer;
    color: #111; }
a.close:hover, .theater .theater-scene a.close:hover {
    text-decoration: none;
    color: #FFF;
    text-shadow: 0 0 2px #000; }

.modal {
    background-color: #FFF;
    box-shadow: 0px 0px 12px -2px #000;
    padding: 15px;
    width: 60%;
    left: 20%;
    position: fixed;
    display: none;
    top: -250px;
    margin: 0 auto;
    z-index: 4000; }
.modal .close, .modal .theater .theater-scene a.close, .theater .theater-scene .modal a.close {
    font-size: 32px;
    position: absolute;
    right: 10px;
    top: 5px;
    color: #888;
    text-shadow: 1px 1px 1px #DDD;
    cursor: pointer; }
.modal .close:hover, .modal .theater .theater-scene a.close:hover, .theater .theater-scene .modal a.close:hover {
    text-decoration: none;
    color: #333333;
    text-shadow: none; }

.out-modal {
    position: fixed;
    display: none;
    z-index: 3999;
    height: 100%;
    width: 100%;
    overflow: hidden;
    background-color: #000;
    background-color: rgba(0, 0, 0, 0.45);
    top: 0;
    left: 0; }

li.block-img, .theater .theater-links li {
    display: flex;
    height: 200px;
    width: 200px;
    box-shadow: 0 0 1px 0px #000;
    transition: all 300ms ease-in;
    margin: 5px;
    background-color: rgba(255, 255, 255, 0.5); }
li.block-img:hover, .theater .theater-links li:hover {
    box-shadow: 0 0 5px 0px #174386; }
li.block-img img, .theater .theater-links li img {
    display: flex;
    height: 200px;
    min-width: 200px;
    clip: rect(10px, 190px, 190px, 10px);
    position: absolute;
    text-align: center;
    margin: 0 auto; }

[theater] {
    display: flex; }
[theater] li {
    cursor: pointer; }

.theater {
    position: fixed;
    display: none;
    z-index: 3999;
    height: 100%;
    width: 100%;
    top: 0;
    left: 0; }
.theater > * {
    display: none;
    position: relative;
    background-color: #333333; }
.theater .theater-scene {
    height: 75%;
    top: -75%; }
.theater .theater-scene img {
    display: flex;
    margin: 0 auto;
    max-height: 90%;
    padding-top: 15px; }
.theater .theater-scene a.close {
    color: #FFF;
    font-size: 32px; }
.theater .theater-links {
    bottom: -25%;
    height: 25%;
    box-shadow: 0 4px 15px -1px #DDD; }
.theater .theater-links li {
    cursor: pointer;
    height: 95%;
    margin: 0.5%; }
.theater .theater-links li img {
    clip: rect(10px, 190px, 175px, 10px); }

.tabs {
    display: table;
    width: 100%; }
.tabs .tab-panel ul {
    display: flex;
    padding: 0; }
.tabs .tab-panel ul li {
    display: flex;
    padding: 2px 8px;
    font-size: 16px;
    cursor: pointer;
    margin: 0;
    transition: background 500ms;
    word-wrap: break-word; }
.tabs .tab-panel ul li:hover {
    background-color: rgba(0, 0, 0, 0.05); }
.tabs .tab-content .content {
    display: none; }
.tabs .tab-content .content.active {
    display: flex; }
.tabs.to-top .tab-panel ul {
    border-bottom: 1px solid #bbbbbb; }
.tabs.to-top .tab-panel ul li {
    border-style: solid;
    border-color: #bbbbbb;
    border-radius: 3px 3px 0 0;
    border-width: 0 0 0 0;
    margin: 0 4px -1px 4px; }
.tabs.to-top .tab-panel ul li.active {
    border-width: 1px 1px 0 1px;
    margin-bottom: -1px;
    background-color: -webkit-linear-gradient(bottom, white, #eeeeee);
    background-color: -moz-linear-gradient(bottom, white, #eeeeee);
    background-color: -o-linear-gradient(bottom, white, #eeeeee);
    background-color: linear-gradient(to bottom, #eeeeee, white); }
.tabs.to-top .tab-panel ul li:first-child {
    margin-left: 0; }
.tabs.to-top .tab-panel ul li:last-child {
    margin-right: 0; }
.tabs.to-bottom .tab-panel ul {
    border-top: 1px solid #bbbbbb; }
.tabs.to-bottom .tab-panel ul li {
    border-style: solid;
    border-color: #bbbbbb;
    border-radius: 0 0 3px 3px;
    border-width: 0 0 0 0;
    margin: -1px 4px 0 4px; }
.tabs.to-bottom .tab-panel ul li.active {
    border-width: 0 1px 1px 1px;
    margin-top: -1px;
    background-color: -webkit-linear-gradient(top, white, #eeeeee);
    background-color: -moz-linear-gradient(top, white, #eeeeee);
    background-color: -o-linear-gradient(top, white, #eeeeee);
    background-color: linear-gradient(to top, #eeeeee, white); }
.tabs.to-bottom .tab-panel ul li:first-child {
    margin-left: 0; }
.tabs.to-bottom .tab-panel ul li:last-child {
    margin-right: 0; }
.tabs.to-left {
    padding: 0 5px;
    margin: 0;
    position: relative;
    min-height: 1px; }
.tabs.to-left > * {
    float: left; }
.tabs.to-left .tab-panel {
    width: 15%; }
.tabs.to-left .tab-panel ul li {
    display: flex;
    width: 100%;
    border-radius: 3px 0 0 3px;
    border-style: solid;
    border-color: #bbbbbb;
    border-width: 0 1px 0 0; }
.tabs.to-left .tab-panel ul li.active {
    margin: 0;
    border-width: 1px 0 1px 1px;
    background-color: -webkit-linear-gradient(right, white, #eeeeee);
    background-color: -moz-linear-gradient(right, white, #eeeeee);
    background-color: -o-linear-gradient(right, white, #eeeeee);
    background-color: linear-gradient(to right, #eeeeee, white); }
.tabs.to-left .tab-content {
    width: 85%;
    padding: 0 10px; }
.tabs.to-left .tab-content .content {
    width: 100%; }
.tabs.to-right {
    padding: 0 5px;
    margin: 0;
    position: relative;
    min-height: 1px; }
.tabs.to-right .tab-panel {
    float: right;
    width: 15%; }
.tabs.to-right .tab-panel ul li {
    display: flex;
    width: 100%;
    border-radius: 0 3px 3px 0;
    border-style: solid;
    border-color: #bbbbbb;
    border-width: 0 0 0 1px; }
.tabs.to-right .tab-panel ul li.active {
    margin: 0;
    border-width: 1px 1px 1px 0;
    background-color: -webkit-linear-gradient(left, white, #eeeeee);
    background-color: -moz-linear-gradient(left, white, #eeeeee);
    background-color: -o-linear-gradient(left, white, #eeeeee);
    background-color: linear-gradient(to left, #eeeeee, white); }
.tabs.to-right .tab-content {
    float: left;
    width: 85%;
    padding: 0 10px; }
.tabs.to-right .tab-content .content {
    width: 100%; }

.affix-block {
    box-shadow: 0 0 5px -1px #000;
    position: relative;
    padding: 3px;
    width: 100%;
    max-width: 150px; }

.accordeon .ac-section {
    border: 1px solid #bbbbbb;
    position: relative;
    padding: 0px 20px;
    margin: 3px auto;
    background-color: #F7F7F7;
    display: flex; }
.accordeon .ac-section .ac-head {
    padding: 5px 0px;
    cursor: pointer; }
.accordeon .ac-section .ac-body {
    border-top: 1px solid #bbbbbb;
    padding: 0;
    display: none; }
.accordeon .ac-section:hover {
    border: 1px solid #777777;
    background-color: #EEE; }
.accordeon .ac-section.active {
    background-color: #FFF; }
.accordeon .ac-section.active .ac-body {
    display: flex; }
.accordeon .ac-section.active:hover {
    border: 1px solid #bbbbbb;
    background-color: inherit; }

.dropdown {
    cursor: pointer; }
.dropdown.button {
    padding-right: 3em; }
.dropdown.button::after {
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 5px 5px 0 5px;
    border-color: #333333 transparent transparent transparent;
    position: absolute;
    content: "";
    display: flex;
    right: 1rem;
    top: 50%;
    margin-top: -0.2em; }

div.dropdown-content {
    padding: 10px; }

.dropdown-content {
    display: none;
    position: absolute;
    border: 1px solid #bbbbbb;
    margin-top: 2px;
    left: 5px;
    background-color: #FFF;
    max-width: 400px;
    padding: 0;
    z-index: 10; }
.dropdown-content li {
    cursor: pointer;
    color: #333; }
.dropdown-content li a {
    padding: 4px 10px;
    display: flex;
    color: inherit; }
.dropdown-content li a:hover {
    text-decoration: none;
    background-color: #EEE; }
